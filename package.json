    "jsdoc": "^3.6.11",
    "better-docs": "^2.7.2",
    "coveralls": "^3.1.1",
    "eslint": "^8.14.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^28.0.3",
    "twilio-run": "^3.4.2"
  },
  "engines": {
    "node": ">=14"
  }
}
